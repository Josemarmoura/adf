{
	"name": "pipeline3",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "*.json",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['menu']['header']"
								},
								"sink": {
									"name": "header",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['menu']['items'][0]['id']"
								},
								"sink": {
									"name": "id",
									"type": "String"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": false
					}
				},
				"inputs": [
					{
						"referenceName": "DS_Json",
						"type": "DatasetReference",
						"parameters": {
							"Container": "raw",
							"Directry": "Academy"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_AZSQL",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "API/GitHUB"
		},
		"annotations": []
	}
}